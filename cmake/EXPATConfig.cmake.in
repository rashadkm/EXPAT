# Compute paths
get_filename_component(EXPAT_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(EXPAT_INCLUDE_DIR "@CONF_INCLUDE_DIRS@" PARENT_SCOPE)

if(NOT TARGET @LIB_NAME@ AND NOT EXPAT_BINARY_DIR)
  include("${EXPAT_CMAKE_DIR}/expatTargets.cmake")
endif()

if(MSVC)
  get_target_property(LIB_LIB_FILENAME @LIB_NAME@ IMPORTED_IMPLIB_@CONFIG_TYPE@)
else()
  get_target_property(LIB_LIB_FILENAME @LIB_NAME@ IMPORTED_LOCATION_@CONFIG_TYPE@)
endif()

set(EXPAT_LIBRARY ${LIB_LIB_FILENAME} PARENT_SCOPE)

set(EXPAT_LIBRARIES ${LIB_LIB_FILENAME} PARENT_SCOPE)

set(EXPAT_FOUND TRUE)
